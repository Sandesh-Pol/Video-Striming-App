node_modules/

# Ignore build outputs
dist/
build/

# Ignore public assets
public/

# Ignore environment variable files
.env
.env.*

# Ignore package lock files
package-lock.json
yarn.lock

# Ignore coverage files
coverage/

# Ignore temporary files
tmp/
temp/

# Ignore generated files
*.min.js
*.min.css
*.map

# Ignore specific configuration files
*.config.js

# Ignore IDE settings
.vscode/
.idea/

# Ignore log files
*.log

# Ignore macOS and Windows system files
.DS_Store
Thumbs.db
